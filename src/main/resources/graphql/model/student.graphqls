type Student {
    id: Int
    name: String
    surname: String
    indexNumber: String!
    version: Int
    createdOn: String
    updatedOn: String
}

input StudentInput {
    id: Int
    name: String!
    surname: String!
    indexNumber: String!
    version: Int
}

type PageStudent {
    content: [Student]
    totalPages: Int
    totalElements: Int
}
